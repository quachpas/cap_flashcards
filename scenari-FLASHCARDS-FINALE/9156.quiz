<sc:textLeaf role="mathtex">760 \, \mathrm{kg \cdot L^{-1}}</sc:textLeaf>.</sc:para>
					</op:txt>
				</sc:choiceLabel>
			</sc:choice>
		</sc:choices>
		<sc:solution choice="1"/>
		<sc:globalExplanation>
			<op:res>
				<sp:txt>
					<op:txt>
						<sc:para xml:space="preserve">
			Par définition de la masse volumique <sc:textLeaf role="mathtex">\rho</sc:textLeaf>, <sc:textLeaf role="mathtex">\rho=\dfrac{m}{V}=\dfrac{0,190}{4} \approx 0,047 \, \mathrm{kg \cdot L^{-1}}</sc:textLeaf>.</sc:para>
					</op:txt>
				</sp:txt>
			</op:res>
