		<sc:question>
			<op:res>
				<sp:txt>
					<!--<comment xmlns="scenari.eu:comment:1.0" type="thread" threadClosed="true"><comment author="fav" creationTime="1467381452102">cf commentaire 9405, uniformiser.</comment></comment>-->
					<op:txt>
						<sc:para xml:space="preserve">
			Déterminer, si elle existe, <sc:textLeaf role="mathtex">\lim\limits_{x\to+\infty}\frac{x^3}{\ln x}</sc:textLeaf>.
		</sc:para>
					</op:txt>
				</sp:txt>
			</op:res>
		</sc:question>
